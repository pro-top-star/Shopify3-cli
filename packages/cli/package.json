{
    "name": "@shopify/shopify-cli",
    "version": "0.5.0",
    "description": "A CLI tool to build for the Shopify platform",
    "keywords": [
      "shopify",
      "shopify-cli",
      "shopify-partners"
    ],
    "license": "MIT",
    "private": false,
    "scripts": {
      "test": "vitest run",
      "test:watch": "vitest",
      "build": "rimraf dist && rollup -c",
      "build:watch": "rollup -c --watch --watch.include=src/**",
      "shopify": "rollup -c --plugin @rollup/plugin-run",
      "lint": "prettier -c src/** && eslint src/**",
      "lint:fix": "prettier src/** && eslint src/** --fix",
      "tsc": "tsc --noEmit"
    },
    "bin": {
      "shopify": "./bin/shopify.mjs"
    },
    "dependencies": {
      "@oclif/core": "^1",
      "@oclif/plugin-help": "^5",
      "@oclif/plugin-plugins": "^2.0.1"
    },
    "devDependencies": {
      "@oclif/test": "^2",
      "eslint-config-oclif": "^4",
      "eslint-config-oclif-typescript": "^1.0.2"
    },
    "peerDependencies": {},
    "files": [
      "dist",
      "bin",
      "*.mjs",
      "oclif.manifest.json"
    ],
    "eslintConfig": {
      "extends": [
        "../../package.json",
        "oclif",
        "oclif-typescript"
      ]
    }
  }
