{
  "name": "@shopify/cli",
  "version": "0.6.0",
  "private": false,
  "description": "A CLI tool to build for the Shopify platform",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "keywords": [
    "shopify",
    "shopify-cli",
    "shopify-partners"
  ],
  "license": "MIT",
  "bin": {
    "shopify": "./bin/shopify-run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "shx rm -rf dist && rollup -c",
    "prepublishOnly": "yarn run build",
    "lint": "prettier -c src/** && eslint src/**",
    "lint:fix": "prettier src/** && eslint src/** --fix",
    "test": "shx rm -rf dist && vitest run",
    "test:watch": "vitest watch",
    "tsc": "tsc -b"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ]
  },
  "dependencies": {
    "@bugsnag/js": "^7.14.1",
    "@oclif/core": "0.5.10",
    "@shopify/core": "0.5.2"
  },
  "devDependencies": {
    "@shopify/core": "0.5.2"
  },
  "engine-strict": true,
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "bin": "shopify",
    "commands": "dist/commands",
    "plugins": []
  }
}
