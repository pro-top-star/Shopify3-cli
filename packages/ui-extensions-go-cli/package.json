{
  "name": "@shopify/ui-extensions-go-cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "node bin/build.js",
    "clean": "lerna run clean",
    "deploy": "lerna publish from-package --yes",
    "start": "cd packages/dev-console-app; yarn start",
    "test": "node bin/test.js",
    "package": "node bin/package.js",
    "version-bump": "lerna version --no-push --include-merged-tags",
    "lint": "gofmt -s .",
    "lint:fix": "gofmt -w -s ."
  },
  "devDependencies": {
    "@shopify/shopify-cli-extensions": "0.2.3"
  }
}

